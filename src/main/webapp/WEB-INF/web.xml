<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>LoginPage</display-name>
  <welcome-file-list>
    <welcome-file>register.html</welcome-file>
    
  </welcome-file-list>
  
  
  <servlet-mapping>
  <servlet-name>RegisterDetails</servlet-name>
  <url-pattern>/registerpage</url-pattern>
  
  </servlet-mapping>
  <servlet>
  <servlet-name>RegisterDetails</servlet-name>
  <servlet-class>com.qsp.LoginPage.RegisterDetails</servlet-class>

 
  </servlet>
  
  <servlet-mapping>
  <servlet-name>LoginDetails</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>LoginDetails</servlet-name>
  <servlet-class>com.qsp.LoginPage.LoginDetails</servlet-class>
  </servlet>
  
 
  
  
   <servlet-mapping>
  <servlet-name>Voting</servlet-name>
  <url-pattern>/voting</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>Voting</servlet-name>
  <servlet-class>com.qsp.LoginPage.Voting</servlet-class>
  
 
  </servlet>
  
  
  
</web-app>